<Window x:Class="AirportPlates.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AirportPlates"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel Orientation="Horizontal">
            <Button Command="{Binding Path=OpenTestCmd}">Open PDF Test</Button>
            <Button Margin="10,0,10,0" Command="{Binding Path=ParsePDFCmd}">Parse PDFs</Button>
            <Button Command="{Binding Path=ConvertImagesCmd}" >Convert PDFs To Images</Button>
        </StackPanel>
    </Grid>
</Window>
